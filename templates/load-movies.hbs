<div class="row">
  {{#each this}}
  <div class="col-md-4">
    <div class="thumbnail">
      <button type="button" class="btn btn-default btn-lg">
        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
      </button>
      <img src="{{Poster}}" alt="">
      <div class="caption">

          <h3>{{Title}}</h3>


          <h4>{{Year}}</h4>


          <h4>{{Type}}</h4>


         <input type="number" class="StarRate" data-stars=5>
        <div class="btn-group">
          <button type="button" id="addToWatch" class="btn btn-danger">ADD</button>
          <button type="button" id="Watched" class="btn btn-primary">WATCHED</button>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
